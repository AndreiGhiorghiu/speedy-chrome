{
  "manifest_version": 3,
  "name": "Speedy",
  "description": "",
  "options_page": "options.html",
  "background": { "service_worker": "background.bundle.js", "type": "module" },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon-34.png"
  },
  "chrome_url_overrides": {},
  "icons": {
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "run_at": "document_end",
      "js": ["contentScript.bundle.js"],
      "css": ["content.styles.css"]
    }
  ],
  "devtools_page": "devtools.html",
  "web_accessible_resources": [
    {
      "resources": [
        "contentScript.bundle.js",
        "content.styles.css",
        "icon-128.png",
        "icon-34.png"
      ],
      "matches": ["<all_urls>"]
    }
  ],
  "host_permissions": ["<all_urls>"],
  "permissions": [
    "tabs",
    "clipboardWrite",
    "storage",
    "activeTab",
    "bookmarks",
    "contextMenus",
    "activeTab",
    "scripting",
    "identity",
    "storage",
    "tabs",
    "activeTab",
    "scripting",
    "downloads"
  ]
}
